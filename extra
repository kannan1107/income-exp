function readAll (){
    var tbtable= document.querySelector(".table_data")
var element ="";
    data.map(d =>(
        element += '<tr> <td>${d.name}</td><td>${d.phone}</td> <td>${d.date}</td><td>${d.income}</td> <td>${d.expance}</td><td>${d.expance}</td> <td>${d.balance}</td></tr>'
    ))
    }

